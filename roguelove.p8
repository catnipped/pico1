pico-8 cartridge // http://www.pico-8.com
version 5
__lua__

plr = {}
plr.x = 10
plr.y = 10
plr.sprite = "@"
plr.color = 7
plr.mapx = 0
plr.mapy = 0

function _init()
  camera(-1,-1)
--generate background
  for x=0,24 do
    for y=0,24 do
      mset(x,y,rnd(5)+1)
    end
  end
  print("generate map ok")
  drawmap()
  print("draw map ok")
  cls()

end

function controls(a)
  if btnp(2) then
    a.y -= 1
  end
  if btnp(3) then
    a.y += 1
  end
  if btnp(0) then
    a.x -= 1
  end
  if btnp(1) then
    a.x += 1
  end
end



function _update()
  controls(plr)
end

function drawmap()
  -- for x = 0,24 do
  --   for y = 0,24 do
  --     for i = 0,8 do
  --       for n = 0,8 do
  --         -- generate wall
  --         if sget(i + n*8 ,y) == 7 then
  --           print("#",col(i+x*8),row(n+y*8),5)
  --         end
  --       end
  --     end
  --
  --   end
  -- end
end

function drawcurrentmap(x,y)
  local z = mget(x,y)
  local mapx = 8 * x
  local mapy = 8 * y
  for i = 0,7 do
        for n = 0,7 do
          -- generate wall
          if sget(z*8+i,n) == 7 then
            print("#",col(i+mapx),row(n+mapy),5)
          end
        end
      end
end

function col(a)
  if a > 0 then a = a-1 end
  return a * 4
end

function row(a)
  if a > 0 then a = a-1 end
  return a * 6
end

function drawsprite(i,n) --entity, shadow true/false
  if n == true then
    print(i.sprite, col(i.x)+1, row(i.y)+1,5)
  end
  print(i.sprite, col(i.x), row(i.y),i.color)
end

function _draw()
  cls()
  --grid
  for x = 1,24 do
    for y = 1,16 do
    pset(col(x)-3,row(y)-4,1)
    end
  end
  rect(-1,-1,col(24)-1,row(16)-1,2)
  for x = 0,2 do
    for y = 0,1 do
      drawcurrentmap(plr.mapx+x,plr.mapy+y)
    end
  end

  drawsprite(plr,true)

end
