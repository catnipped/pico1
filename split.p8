pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
p = {}
timeline = {}
timeline_actor = {}
timeline_counter = 1
timeuntilturn = 0
cam = {}
cam[1] = {}
cam[1].x = 0
cam[1].y = 0
cam[2] = {}
cam[2].x = 0
cam[2].y = 0
camspeed = 0.2

forward = function (m)
	if 			p[m].dir == 0 then p[m].y -= 8
	elseif  p[m].dir == 1 then p[m].x -= 8
	elseif  p[m].dir == 2 then p[m].y += 8
	elseif  p[m].dir == 3 then p[m].x += 8
	end
end

backward = function (m)
	if 			p[m].dir == 0 then p[m].y += 8
	elseif  p[m].dir == 1 then p[m].x += 8
	elseif  p[m].dir == 2 then p[m].y -= 8
	elseif  p[m].dir == 3 then p[m].x -= 8
	end
end

turnr = function (m)
	p[m].dir += 1
	if p[m].dir > 3 then p[m].dir = 0 end
end

turnl = function (m)
	p[m].dir -= 1
	if p[m].dir < 0 then p[m].dir = 3 end
end

uturn = function (m)
	p[m].dir += 1
	if p[m].dir > 3 then p[m].dir = 0 end
	p[m].dir += 1
	if p[m].dir > 3 then p[m].dir = 0 end
end

fast = function (m)
	if 			p[m].dir == 0 then p[m].y -= 16
	elseif  p[m].dir == 1 then p[m].x -= 16
	elseif  p[m].dir == 2 then p[m].y += 16
	elseif  p[m].dir == 3 then p[m].x += 16
	end
end

function lerp(a,b,t)
  return a + t*(b-a)
end

function _init()
	action = {forward,backward,turnr,turnl,uturn,fast}

	p[1] = {}
	p[1].x = 32
	p[1].y = 32
	p[1].dir = 0
	p[1].load = 0

	p[2] = {}
	p[2].x = 64
	p[2].y = 64
	p[2].dir = 0
	p[2].load = 0

	palt(12,true)
	palt(0,false)

	card = {}
	for a = 0,3 do
		card[a] = flr(rnd(count(action)))+1
	end

end

function resetcards()
	for a = 0,3 do
		card[a] = flr(rnd(count(action)))+1
	end
end

function resettimeline()
	timeline = {}
	timeline_actor = {}
	timeline_counter = 0
end

function activator()
	if timeline_counter > count(timeline) then
		 for m = 1,2 do p[m].load = 0 end
		 resetcards()
		--  resettimeline()
		return
	elseif timeuntilturn == 30 then
		x = timeline_actor[timeline_counter]

		timeline[timeline_counter](x)
		timeline_counter += 1
		timeuntilturn = 0
	else
		timeuntilturn += 1
	end
end

function _update()
	for m = 1,2 do
		if btn(5,m-1) then
			if btn(0,m-1) then cam[m].x -= 5 end
			if btn(1,m-1) then cam[m].x += 5 end
			if btn(2,m-1) then cam[m].y -= 5 end
			if btn(3,m-1) then cam[m].y += 5 end
		else
			for c = 0,3 do
				if btnp(c,m-1) and p[m].load <= 3 then
					add(timeline,action[card[c]])
					add(timeline_actor,m)
					p[m].load += 1
				end
			end

			if btnp(4,m-1) then
				resetcards()
			end

			if p[m].load > 3 then activator() end
		end

	end

	if btn(5,0) ~= true then
		cam[1].x = lerp(cam[1].x, p[1].x-28, camspeed)
		cam[1].y = lerp(cam[1].y, p[1].y-64, camspeed)
	end
	if btn(5,1) ~= true then
		cam[2].x = lerp(cam[2].x, p[2].x-92, camspeed)
		cam[2].y = lerp(cam[2].y, p[2].y-64, camspeed)
	end
end

function _draw()
	cls()
	for m = 1,2 do
		if m == 2 then clip(0,0,64,128) end
		if m == 1 then clip(64,0,128,128) end
		map(0,0,0,0,24,24)
		for m = 1,2 do
			rectfill(p[m].x,p[m].y,p[m].x+7,p[m].y+7,9+m) --body
			--drill
			if p[m].dir == 0 then spr(2,p[m].x,p[m].y-8)
			elseif p[m].dir == 2 then spr(2,p[m].x,p[m].y+8,1,1,false,true)
			elseif p[m].dir == 1 then spr(3,p[m].x+8,p[m].y)
			elseif p[m].dir == 3 then spr(3,p[m].x-8,p[m].y,1,1,true)
			end
		end


		camera(cam[m].x,cam[m].y)
	end

	clip()
	for a = 1,4 do
		cardx = cam[2].x+30*a-22
		cardy = cam[2].y+100
		rectfill(cardx,cardy,cardx+24,cardy+30,0)
		rect(cardx,cardy,cardx+24,cardy+30,7)
		rectfill(cardx,cardy,cardx+7,cardy+7,7)
		spr(3+a,cardx,cardy)
		spr(16+card[a-1], cardx+9, cardy+12)
	end

	print(count(timeline),cam[2].x+1,cam[2].y+1,9)
end


