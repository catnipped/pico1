pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
p = {}
timeline = {}


forward = function (m)
	if 			p[m].dir == 0 then p[m].y -= 8
	elseif  p[m].dir == 1 then p[m].x -= 8
	elseif  p[m].dir == 2 then p[m].y += 8
	elseif  p[m].dir == 3 then p[m].x += 8
	end
end

backward = function (m)
	if 			p[m].dir == 0 then p[m].y += 8
	elseif  p[m].dir == 1 then p[m].x += 8
	elseif  p[m].dir == 2 then p[m].y -= 8
	elseif  p[m].dir == 3 then p[m].x -= 8
	end
end

turnr = function (m)
	p[m].dir += 1
	if p[m].dir > 3 then p[m].dir = 0 end
end

turnl = function (m)
	p[m].dir -= 1
	if p[m].dir < 0 then p[m].dir = 3 end
end

uturn = function (m)
	p[m].dir += 1
	if p[m].dir > 3 then p[m].dir = 0 end
	p[m].dir += 1
	if p[m].dir > 3 then p[m].dir = 0 end
end

fast = function (m)
	if 			p[m].dir == 0 then p[m].y -= 16
	elseif  p[m].dir == 1 then p[m].x -= 16
	elseif  p[m].dir == 2 then p[m].y += 16
	elseif  p[m].dir == 3 then p[m].x += 16
	end
end

function lerp(a,b,t)
  return a + t*(b-a)
end

function _init()
	action = {forward,backward,turnr,turnl,uturn,fast}
	p[1] = {}
	p[1].x = 32
	p[1].y = 32
	p[1].dir = 0
	p[1].inst = {}
	p[1].load = 0
	p[2] = {}
	p[2].x = -64
	p[2].y = 0
	p[2].dir = 0
	p[2].load = 0
	p[2].inst = {}
	palt(12,true)
	palt(0,false)
	card = {}
	for a = 0,3 do
		card[a] = flr(rnd(count(action)))+1
	end
end

function _update()
	for m = 1,2 do

		for c = 0,3 do
			if btnp(c,m-1) and p[m].load <= 3 then
				add(timeline,action[card[c]])
				p[m].load += 1
			end
		end

		if btnp(4,m-1) then
			timeline[1](m)
		end
	end
end

function _draw()
	cls()
	for m = 1,2 do
		split = 64 * (m-1)
		clip(split,0,64+split,128)
		map(0,0,0,0)
		camera(p[m].x-32,p[m].y-64)
		rectfill(p[m].x,p[m].y,p[m].x+7,p[m].y+7,9) --body
		--drill
		if p[m].dir == 0 then spr(2,p[m].x,p[m].y-8)
		elseif p[m].dir == 2 then spr(2,p[m].x,p[m].y+8,1,1,false,true)
		elseif p[m].dir == 1 then spr(3,p[m].x+8,p[m].y)
		elseif p[m].dir == 3 then spr(3,p[m].x-8,p[m].y,1,1,true)
		end

		print(count(timeline),5,10,9)
	end

	clip()
	camera(0,0)
	for a = 1,4 do
		cardx = 30*a-24
		cardy = 100
		rectfill(cardx,cardy,cardx+24,cardy+30,5)
		rect(cardx,cardy,cardx+24,cardy+30,7)
		rectfill(cardx,cardy,cardx+7,cardy+7,7)
		spr(3+a,cardx,cardy)
		spr(16+card[a-1], cardx+9, cardy+12)
	end
end


