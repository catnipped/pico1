pico-8 cartridge // http://www.pico-8.com
version 7
__lua__

p = {}
cam = {}
cam.x = 0
cam.y = 0
cam.speed = 0.04

function every(a,b) -- a: number of frames true b: 1-30 where 1 is every frame, 30 is once each second.
  return frames % b < a
end

function makeplayer(nr)
  p[nr] = {}
  p[nr].x = rnd(64)
  p[nr].y = 8*60
  p[nr].face = "right"
  p[nr].stance = "standing"
  p[nr].grounded = true
  p[nr].fy = 0
  p[nr].fx = 0
  p[nr].fromx = 0
  p[nr].fromy = 0
  p[nr].id = nr-1
  p[nr].tandem = 0
end

function _init()
  frames = 0
  makeplayer(1)
  makeplayer(2)
  palt(0,false)
  palt(1,true)
end

function flr8(v)
 return flr(v/8)
end

function lerp(a,b,t)
  return a+t*(b-a)
end

function msget(x,y)
 return mget(flr8(x),flr8(y))
end

function collides(x,y)
 if(x<1) return true--left edge of map
 if(x>120) return true
 if msget(x,y)==17 or msget(x+7,y)==17 then return true
 else
  ycol=8
  if msget(x,y-ycol)==17 or msget(x+7,y-ycol)==17 then return true end
  return false
 end
end

function moveplayer(p)
 p.fromx=p.x
 p.fromy=p.y

 p.x+=p.fx
 p.y+=p.fy

 --foot collision
  if collides(p.x,p.y) then
    if collides(p.fromx,p.y) then
     --collides even on old x
     --floor or ceiling collision
     p.x=p.fromx
     p.y=p.fromy
     p.fy=0
    else
     if collides(p.x,p.fromy) then
      --collides even on old y
      --wall collision
      p.fx *= -0.7
      p.x = p.fromx
     end
    end

    if collides(p.x,p.y) then
     --still collides, it was
     --a complete collision
     p.x=p.fromx
     p.y=p.fromy
     p.fx=0
     p.fy=0
    end
  end
end

function controlplayer(p)
  if btn(0,p.id) == true then
    p.stance = "walking"
    p.face = "left"
    p.fx -= 0.5
  elseif btn(1,p.id) == true then
    p.stance = "walking"
    p.face = "right"
    p.fx += 0.5
  elseif p.stance ~= "jumping" then
    p.stance = "standing"
  end

  if collides(p.x,p.y+1)!=true then
   --not grounded
   p.grounded = false
   p.fy+=0.5
  else
   --grounded
   p.grounded = true
   p.stance = "standing"
   p.tandem = true
   if btnp(4,p.id) then --jump!
    p.fy=-7
    p.stance = "jumping"
   end
  end

  --forces
  if p.fx>0 then
   p.fx=max(p.fx-0.2,0)
  else
   p.fx=min(p.fx+0.2,0)
  end
  p.fx=mid(-3,p.fx,3)
  p.fy=mid(-10,p.fy,5)
end

function tandemjump(a,b)
  if a.grounded == false and b.grounded == false and a.tandem == true and btnp(4,b.id) then --jump!
   b.fy=-7
   b.stance = "jumping"
   a.tandem = false
  end
end

function _update()
  frames += 1

  controlplayer(p[1])
  controlplayer(p[2])
  tandemjump(p[1],p[2])
  tandemjump(p[2],p[1])
  moveplayer(p[1])
  moveplayer(p[2])
  cam.x = 0
  cam.y = lerp(cam.y, lerp(min(p[1].y,p[2].y),max(p[1].y,p[2].y),0.4),cam.speed)
  cam.y = mid(0,cam.y,64*7)
end

function drawplayer(p,x,y)
 local wobble = 1
 local anim = 0
 local jump = 0
  if p.stance == "walking" then
    if every(4,8) == true then wobble = 1 else wobble = 0 end
    if every(3,6) == true then anim = 1 else anim = 2 end
  end

  if p.stance == "jumping" then jump = 1 anim = 3 end

  if p.face == "right" then
    spr(2+anim,x,y-7) --body
    spr(1,x+wobble,y-11-jump) --head
  elseif p.face == "left" then
    spr(2+anim,x,y-7,1,1,true) --body
    spr(1,x-wobble,y-11-jump,1,1,true) --head
  end

end

function _draw()
  cls()
  camera(cam.x,cam.y-64)
  rectfill(0,0,128,128*8,3)
  sspr(16,8,8,8,cam.x-6,cam.y-70,136,136)
  --shadows
  shadow = 2
  pal(7,0)
  map(0,0,shadow,shadow,16,64)
  drawplayer(p[1],p[1].x+shadow,p[1].y+shadow)
  drawplayer(p[2],p[2].x+shadow,p[2].y+shadow)
  pal(7,7)
  --real stuff
  map(0,0,0,0,16,64)
  drawplayer(p[1],p[1].x,p[1].y)
  drawplayer(p[2],p[2].x,p[2].y)
  for nr = 1,2 do
    if p[nr].grounded == true and btnp(4,p[nr].id) then
      spr(6,p[nr].x,p[nr].y)
    end
  end

  if p[1].tandem == true and p[1].grounded == false and p[2].grounded == false then spr(19,p[1].x+12,p[1].y-12,2,1) end
  if p[2].tandem == true and p[2].grounded == false and p[1].grounded == false then spr(19,p[2].x+12,p[2].y-12,2,1) end

end

