d6 = {}
d6[1] = {2,4,5,3}
d6[2] = {1,3,6,4}
d6[3] = {1,5,6,2}
d6[4] = {1,2,6,5}
d6[5] = {1,4,6,3}
d6[6] = {2,3,5,4}

faces = {}
faces[1] = 5
faces[2] = 7
faces[3] = 37
faces[4] = 39

cam = {x = 0, y = 0}
player = {}

hand = 33
hold = 44
clock = 35
player[1] = { x = 1, y = 1, sprite = hand, color = faces[1]}
player[2] = { x = 6, y = 6, sprite = clock, color = faces[2]}
dice = {}
dice[1] = {}
dice[2] = {}

anim_x = 1
anim_x_invert = 0
anim_y = 1
anim_y_invert = 0

bubble_counter = 0
bubble_true = true
bubble_message = "hello world"
empty = 0
titlefx = {}
titlefx[1] = {x = flr(rnd(128)), y = flr(rnd(128)), sprite = faces[flr(rnd(4))+1]}

function get_d6(from,to)
  local list = {}
  local part1 = {}
  local part2 = {}
  local next_part = false
  for n in all(d6[to]) do
    if n == from then next_part = true end
    if next_part then
      add(part2,n)
    else
      add(part1,n)
    end
  end
  add(list,part2)
  add(list,part1)
  return list
end

function init_dice(player_number,y)
  for n = 1,6 do
    local die = {}
    die.face = n
    die.neighbours = d6[n]
    if player_number == 2 then
      add(die.neighbours,die.neighbours[1])
      add(die.neighbours,die.neighbours[2])
      del(die.neighbours,die.neighbours[1])
      del(die.neighbours,die.neighbours[1])
    end
    die.x = n
    die.y = y
    dice[player_number][n] = die
  end 
end

function draw_dice(p,n)
  local x = dice[p][n].x
  local y = dice[p][n].y
  local sprite = (dice[p][n].face) * 16
  local face_x = {}
  face_x[1] = (x*16)+16*anim_x_invert
  face_x[2] = 16*anim_x
  local face_y = {}
  face_y[1] = (y*16)+16*anim_y_invert
  face_y[2] = 16*anim_y

  --face
  sspr(sprite+8,0,16,16,face_x[1],face_y[1],face_x[2],face_y[2])

  --neighbours
   local west_x = {}
   west_x[1] = x*16
   west_x[2] = 16*anim_x
   local west_y = {}
   west_y[1] = (y*16)+16*anim_y_invert
   west_y[2] = 16
   local sprite = dice[p][n].neighbours * 16
   sspr((16*d6[n.face][1])-8,16,16,west_x[1],west_y[1],west_x[2],west_y[2])
end

function init_grid()
  grid = {}
  for x = 1,8 do
    for y = 1,8 do
      local space = {}
      space.x = x
      space.y = y
      space.pawns = {empty}
      space.access = true
      add(grid,space)
    end
  end

  grid[contains_xy(grid,1,1)].access = false
  grid[contains_xy(grid,2,1)].access = false
  grid[contains_xy(grid,1,2)].access = false
  grid[contains_xy(grid,7,1)].access = false
  grid[contains_xy(grid,8,1)].access = false
  grid[contains_xy(grid,8,2)].access = false
  grid[contains_xy(grid,1,7)].access = false
  grid[contains_xy(grid,1,8)].access = false
  grid[contains_xy(grid,2,8)].access = false
  grid[contains_xy(grid,7,8)].access = false
  grid[contains_xy(grid,8,8)].access = false
  grid[contains_xy(grid,8,7)].access = false

  seed_board()
end

function seed_board()
  for x = 2,7 do
    for y = 2,7 do
      grid[contains_xy(grid,x,y)].pawns[1] = player[2].color
      if x == 2 or x == 4 or x == 6 then
        if y == 2 or y == 3 or y == 6 or y == 7 then
          grid[contains_xy(grid,x,y)].pawns[1] = player[1].color
          grid[contains_xy(grid,x,y)].pawns[2] = player[2].color
          grid[contains_xy(grid,x,y)].pawns[3] = player[1].color
          grid[contains_xy(grid,x,y)].pawns[4] = player[2].color
          grid[contains_xy(grid,x,y)].pawns[5] = player[1].color
        end
      elseif x== 3 or x == 5 or x == 7 then
        if y == 4 or y == 5 then
          grid[contains_xy(grid,x,y)].pawns[1] = player[1].color
        end
      end
    end
  end
end


function contains_xy(table, x, y)
   for i=1,#table do
      if table[i].x == x and table[i].y == y then
         return i
      end
   end
   return 0
end

function lerp(a,b,t)
  return a + t*(b-a)
end

function rotlerp(a, b, t)
   while a < 0 do
      a = a + 1
   end
   while b < 0 do
      b = b + 1
   end
   diff = b - a
   if diff > 0.5 then
      b -= 1
   elseif diff < -0.5 then
      b += 1
   end
   return a + t * (b - a)
end

function every(duration,offset,period)
  local offset = offset or 0
  local period = period or 1
  local offset_frames = frames + offset
  return offset_frames % duration < period
end

function splash()
  palt(1,true)
  palt(0,false)
  if frames > 45 then lid.y = lerp(lid.y,trash.y,0.08) end
  if flr(lid.y) == trash.y then lid.spr = 196 end
  if frames < 200 then
    rectfill(0,0,128,128,10)
    spr(192,trash.x,trash.y,4,4)
    spr(lid.spr,lid.x,lid.y,4,2)
  end
  if frames > 55 and trash.sfx then
      sfx(0,0)
      trash.sfx = false
  end
  if frames > 75 and frames < 220 then
    line(trash.x+13,trash.y+8,trash.x+13,trash.y+9,10)
    line(trash.x+18,trash.y+8,trash.x+18,trash.y+9,10)

    local c = 200
    local a = 201
    local t = 202
    local n = 203
    local i = 216
    local p = 217
    local d = 218
    local e = 219
    local x = 34
    local y = trash.y + 32
    spr(c,x,y)
    spr(a,x+1*7,y)
    spr(t,x+2*7,y)
    spr(n,x+3*7,y)
    spr(i,x+4*7-1,y)
    spr(p,x+5*7-2,y)
    spr(p,x+6*7-2,y)
    spr(e,x+7*7-2,y)
    spr(d,x+8*7-2,y)
  end
  if frames > 260 then trash.done = true end
  palt()
end

function init_splash()
  trash = {}
  trash.x = 48
  trash.y = 44
  trash.sfx = true
  trash.done = false
  lid = {}
  lid.spr = 228
  lid.x = trash.x
  lid.y = trash.y+2
end

function init_background()
  for x = 16,32 do
    for y = 0,16 do
      mset(x,y,9+flr(rnd(7)))
    end
  end
  palt(0,false)
  palt(1,true)
end

function draw_board()
  camera(cam.x,cam.y)
  map(16,0,cam.x,cam.y,16,16)
  map(0,0,0,0,16,16)
  for a = 1, #grid do
    if #grid[a].pawns > 0 then
      for z = 1,#grid[a].pawns do
        local sprite = grid[a].pawns[z]
        if sprite ~= 0 then
          local x = grid[a].x-1
          local y = grid[a].y-1
          spr(sprite,x*16,y*16-(z-1)*3-1,2,2)
        end
      end
    end
  end

  local x = player[1].x
  local y = player[1].y
  if player[1].sprite == hold then
    local offset_y = #grid[contains_xy(grid,x,y)]
    for s in all(player[1].stack) do
      spr(s,(x-1)*16,((y-1)*16)-offset_y*3,2,2)
      offset_y += 1
    end
    spr(player[1].sprite,(x-1)*16,(y-1)*16-offset_y*3,2,2)
  else
    spr(player[1].sprite,(x-1)*16,(y-1)*16,2,2)
  end

end

function draw_gui()
  local x = cam.x
  local y = cam.y
  rectfill(x+32,y+128-7,x+96,y+128,0)
  print("x:pick o:place",x+33,y+128-6,7)

  if bubble_true then
    bubble(x+14,y+96,bubble_message)
    bubble_counter += 0.7
  else
    bubble_counter = 0
  end
end

function bubble(x,y,message)
  local string = sub(message,1,flr(bubble_counter))
  local width = mid(16,#string*4,76)
  local height = 0
  if #string > 19 then height = (flr(#string/19))*8 end

  rectfill(x,y-height,x+width+4,y+10,7)
  rect(x,y-height,x+width+4,y+10,0)
  spr(43,x+8,y+10)
  spr(41,x-8,y+12,2,2)

  local skip = 0
  print(sub(string,1,19),x+3,y-height+3,0)
  if sub(string,20,20) == " " then skip += 1 end
  print(sub(string,20+skip,38+skip),x+3,y-height+11,0)
  if sub(string,40-skip,40-skip) == " " then skip += 1 end
  print(sub(string,39+skip,57+skip),x+3,y-height+19,0)
  if sub(string,60-skip,60-skip) == " " then skip += 1 end
  print(sub(string,58+skip,76+skip),x+3,y-height+27,0)
end

function draw_title()
  camera(0,0)
  map(16,0,0,0,16,16)
  if every(flr(rnd(120))) and #titlefx < 100 then
    titlefx[#titlefx+1] = {x = flr(rnd(128)), y = flr(rnd(128)), sprite = faces[flr(rnd(4))+1]}
  end
  for a = 1,#titlefx do
    local sprite = titlefx[a].sprite
    local x = titlefx[a].x
    local y = titlefx[a].y
    local offset_x = sin((frames+(a*10))/150)*(10+rnd(1))
    local offset_y = cos((frames+(-a*10))/70)*(3+rnd(1))
    spr(sprite,x+offset_x,y+offset_y,2,2)
  end

  for a = 1,8 do
    local offset = sin((frames+(a*10))/50)*2.1
    local x = (a-1)*16
    local y = 48+offset
    local sprite = 62 + a*2
    spr(sprite,x,y,2,2)
  end
  rectfill(48,71,78,78,0)
  print("a game",52,72,7)
end

function pickup(player)
  x = player.x
  y = player.y
  stack = grid[contains_xy(grid,x,y)].pawns
  if stack[#stack] == player.color then
    player.sprite = hold
    add(player.stack,stack[#stack])
    del(stack,stack[#stack])
  end
end

function _init()
  frames = 0
  init_splash()
  init_background()
  init_grid()
  init_dice(1,6)
  init_dice(2,1)
end

function _update()
  frames += 1
  local a = 1
  cam.x = lerp(cam.x,lerp(player[a].x*16,64,0.3)-56,0.1)
  cam.y = lerp(cam.y,lerp(player[a].y*16,64,0.3)-56,0.1)

  if btnp(0,0) then player[1].x -= 1 end
  if btnp(1,0) then player[1].x += 1 end
  if btnp(3,0) then player[1].y += 1 end
  if btnp(2,0) then player[1].y -= 1 end
  if btnp(4,0) then pickup(player[1]) end
end

function _draw()
  cls()
  for n = 1,#dice[1] do
    draw_dice(1,n)
  end
  for n = 1,#dice[2] do
    draw_dice(2,n)
  end
  --draw_title()
  --draw_board()
  --draw_gui()
  --  splash()
  print(dice[1][5].x,5,5,8)
end

